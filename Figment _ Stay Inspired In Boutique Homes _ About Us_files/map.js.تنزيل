var new_map,markers=[],activeIcon="https://www.figment.live/wp-content/themes/figment/img/Pin.svg",inactiveIcon="https://www.figment.live/wp-content/themes/figment/img/Pin-inactive.svg";function initSuitesMap(){function t(){$.each(markers,function(){this.setIcon(inactiveIcon)});const t=scripts.isMobile()?"#selected-suite-mobile":"#selected-suite";this.setIcon(activeIcon);var e=this.suiteId;const i=$(".map__suite").hide().filter(function(){return $(this).attr("data-id")===e}).children().clone();$(t).html("").append(i),scripts.verticalSuiteSlider()}
new_map=new google.maps.Map(document.getElementById("suites-map"),{center:{lat:-25.348,lng:131.039},zoom:15,fullscreenControl:!1}),$(".map__suite").each(function(){var e=$(this),i=new google.maps.Marker({suiteId:e.attr("data-id"),position:{lat:+e.attr("data-lat"),lng:+e.attr("data-lng")},icon:inactiveIcon,map:map});i.addListener("click",t),markers.push(i)})}