if(jQuery(this).hasClass('homeowners_page')){typeformpopup_homeowner.open()}else if(jQuery(this).hasClass('sale_page')){typeformpopup_sale.open()}else if(jQuery(this).hasClass('referral_page')){typeformpopup_referral.open()}else if(jQuery(this).hasClass('join_figment_typeform_btn')){typeformpopup_join_figment.open()}else{typeformpopup.open()}});jQuery(function($){function changed(){console.log('Question changed')}
$('#book-a-viewing').on('click',function(){if($(".feedback").is("#form")){$("html, body").animate({scrollTop:$('#form').offset().top-50},500)}else{location='<?php echo site_url("/#form"); ?>'}});$('.form').on('keyup keypress',function(e){var keyCode=e.keyCode||e.which;if(keyCode===13){e.preventDefault();return!1}});function isCalendlyEvent(e){return e.data.event&&e.data.event.indexOf('calendly')===0};window.addEventListener('message',function(e){if(isCalendlyEvent(e)){if(e.data.event=='calendly.event_type_viewed'){}else if(e.data.event=='calendly.event_scheduled'){}}})});jQuery(document).ready(function(){jQuery('#custome-popup-form').on('click',function(){jQuery('.custome-popup').show()});jQuery('.popup-close').on('click',function(){jQuery('.custome-popup').hide()})});jQuery("a.view-iframe").click(function(){var iframesrc=jQuery(this).attr('data-iframesrc');jQuery('body').addClass('active_iframe');if(iframesrc!=''){jQuery('.iframe_popup .iframeWrapper iframe').attr('src',iframesrc);jQuery('.iframe_popup').addClass('show')}});jQuery(".iframe_popup .closed_iframe a").click(function(){jQuery('body').removeClass('active_iframe');jQuery('.iframe_popup').removeClass('show');jQuery('.iframe_popup .iframeWrapper iframe').attr('src','')})